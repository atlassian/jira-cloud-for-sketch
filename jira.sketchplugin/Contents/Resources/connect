var _prodInvariant = __webpack_require__(7),
    _assign = __webpack_require__(10);

var AutoFocusUtils = __webpack_require__(523);
var CSSPropertyOperations = __webpack_require__(525);
var DOMLazyTree = __webpack_require__(79);
